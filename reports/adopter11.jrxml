where upper(prod_name) like '%TISSUE%'
and org_id = 3
and v.Prod_ID = p.prod_id
and c.ver_id = v.ver_id
and a.comp_id = c.comp_id
and x.ta_id = a.ta_id
and tc.auto_id = x.auto_id
